<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>

<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.SplitPane?>

<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<?import javafx.collections.FXCollections?>

<fx:root type="net.calebscode.langforge.app.phonology.view.PhonologyView" xmlns:fx="http://javafx.com/fxml">
	<SplitPane AnchorPane.topAnchor="0" AnchorPane.bottomAnchor="0" AnchorPane.leftAnchor="0" AnchorPane.rightAnchor="0">
		<ListView fx:id="viewSelector" minWidth="150" maxWidth="150">
			<items>
				<FXCollections fx:factory="observableArrayList">
					<String fx:value="Phonological Inventory" />
					<String fx:value="Syllable Patterns" />
				</FXCollections>
			</items>
		</ListView>
		<VBox fx:id="contentContainer">
		</VBox>
	</SplitPane>
</fx:root>
