<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>

<?import javafx.collections.FXCollections?>

<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.Button?>

<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.GridPane?>

<?import net.calebscode.langforge.phonology.phoneme.StandardPhonemeFeatures?>

<fx:root type="net.calebscode.langforge.app.phonology.view.IpaVowelPicker" xmlns:fx="http://javafx.com/fxml">
	<TableView fx:id="vowelTable"
		AnchorPane.topAnchor="0" AnchorPane.bottomAnchor="0" AnchorPane.leftAnchor="0" AnchorPane.rightAnchor="0">
		<columnResizePolicy>
			<TableView fx:constant="CONSTRAINED_RESIZE_POLICY_NEXT_COLUMN"/>
		</columnResizePolicy>
		<columns>
			<TableColumn fx:id="columnOpenness" minWidth="100" />
			<TableColumn fx:id="columnFront" text="Front" />
			<TableColumn fx:id="columnCentral" text="Central" />
			<TableColumn fx:id="columnBack" text="Back" />
		</columns>
	</TableView>
</fx:root>
