<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>

<?import javafx.collections.FXCollections?>

<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.Button?>

<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.GridPane?>

<?import net.calebscode.langforge.phonology.phoneme.StandardPhonemeFeatures?>

<fx:root type="net.calebscode.langforge.app.phonology.view.IpaConsonantPicker" xmlns:fx="http://javafx.com/fxml">
	<TableView fx:id="ipaTable"
		AnchorPane.topAnchor="0" AnchorPane.bottomAnchor="0" AnchorPane.leftAnchor="0" AnchorPane.rightAnchor="0">
		<columnResizePolicy>
			<TableView fx:constant="CONSTRAINED_RESIZE_POLICY_NEXT_COLUMN"/>
		</columnResizePolicy>
		<columns>
			<TableColumn fx:id="columnType" minWidth="100" />
			<TableColumn fx:id="columnBilabial" text="Bilabial" />
			<TableColumn fx:id="columnLabiodental" text="Labiodental" />
			<TableColumn fx:id="columnDental" text="Dental" />
			<TableColumn fx:id="columnAlveolar" text="Alveolar" />
			<TableColumn fx:id="columnPostalveolar" text="Postalveolar" />
			<TableColumn fx:id="columnRetroflex" text="Retroflex" />
			<TableColumn fx:id="columnPalatal" text="Palatal" />
			<TableColumn fx:id="columnVelar" text="Velar" />
			<TableColumn fx:id="columnUvular" text="Uvular" />
			<TableColumn fx:id="columnPharyngeal" text="Pharyngeal" />
			<TableColumn fx:id="columnGlottal" text="Glottal" />
		</columns>
	</TableView>
</fx:root>
